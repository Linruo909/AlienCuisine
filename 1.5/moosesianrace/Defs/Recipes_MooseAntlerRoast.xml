<Defs>
	<!-- 鹿茸烤肉配方 -->
    <RecipeDef MayRequire="waffelf.moosesianrace">
        <defName>AC_CookMooseAntlerRoast</defName>
        <label>Cook Moose Antler Roast</label>
        <description>A hearty roast made from Moosesian meat, known for its unique flavor and health benefits.</description>
        <jobString>Cooking Moose Antler Roast.</jobString>
        <workSpeedStat>CookSpeed</workSpeedStat>
        <effectWorking>Cook</effectWorking>
        <soundWorking>Recipe_CookMeal</soundWorking>
        <allowMixingIngredients>true</allowMixingIngredients>
        <fixedIngredientFilter Inherit="false" />
        <recipeUsers>
            <li>ElectricStove</li>
            <li>FueledStove</li>
            <li MayRequire="Solaris.FurnitureBase">RGK_FueledStove</li>
            <li MayRequire="Solaris.FurnitureBase">GL_ElectricStove</li>
            <li MayRequire="Solaris.FurnitureBase">GL_Bonfire</li>
        </recipeUsers>
        <workSkill>Cooking</workSkill>
        <defaultIngredientFilter>
            <thingDefs Inherit="false">
                <li>Meat_Moosesian</li>
            </thingDefs>
        </defaultIngredientFilter>
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Meat_Moosesian</li>
                    </thingDefs>
                </filter>
                <count>15</count>
            </li>
        </ingredients>
        <products>
            <AC_MooseAntlerRoast>1</AC_MooseAntlerRoast>
        </products>
        <skillRequirements>
            <Cooking>4</Cooking>
        </skillRequirements>
    </RecipeDef>

</Defs>